{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "../../publicMethods/**/*",
    "../../types/*"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "allowJs": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "rootDir": "../../",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@root/*": [
        "../../*"
      ]
    }
  }
}